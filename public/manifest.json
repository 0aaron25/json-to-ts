{
  "manifest_version": 3,
  "name": "JSON to TS Type Generator",
  "version": "1.0",
  "description": "Generate TypeScript types from JSON data",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
